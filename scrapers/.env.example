# API Settings
DEBUG=false
LOG_LEVEL=INFO

# External APIs
GOOGLE_API_KEY=your_google_api_key_here
NEWS_API_KEY=your_news_api_key_here
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
YOUTUBE_API_KEY=your_youtube_api_key_here

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=60

# Timeouts
HTTP_TIMEOUT=30
SCRAPING_TIMEOUT=300

# ==================== Enhanced Scraping Settings ====================

# Proxy Provider (bright_data, scraper_api, smart_proxy, oxylabs, static)
PROXY_PROVIDER=
PROXY_API_KEY=
# Format: ip:port ou ip:port:username:password
STATIC_PROXIES=

# CAPTCHA Solving (2captcha, anticaptcha)
CAPTCHA_API_KEY=

# Playwright Settings
PLAYWRIGHT_BROWSERS_PATH=/ms-playwright
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_STEALTH=true
